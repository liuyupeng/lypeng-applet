<!--pages/message/message.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="{{msg_info.icons}}" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{msg_info.title}}</view>
            <view class="weui-msg__desc">{{msg_info.context}}</view>
        </view>
        
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="" class="weui-footer__link">{{globalData.foot_text}}</navigator>
                </view>
                <view class="weui-footer__text">Copyright Â© {{globalData.copyright}}</view>
            </view>
        </view>
    </view>
</view>
